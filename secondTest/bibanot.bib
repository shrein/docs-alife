@ARTICLE{Capi02Optimal,
  author = {Capi, G. and Kaneko, S. and Mitobe, K. and Barolli, L. and Nasu,
	Y.},
  title = {Optimal trajectory generation for a prismatic joint biped robot using
	genetic algorithms},
  journal = {Rob Autom Syst},
  year = {2002},
  volume = {38},
  pages = {119--128},
  number = {2},
  abstract = {In this paper, a prismatic joint biped robot trajectory planning method
	is proposed. The minimum consumed energy is used as a criterion for
	trajectory generation, by using a real number genetic algorithm as
	an optimization tool. The minimum torque change cost function and
	constant vertical position trajectories are used in order to compare
	the results and verify the effectiveness of this method. The minimum
	consumed energy walking is stable and the impact of the foot with
	the ground is very small. Experimental investigations of a prismatic
	joint biped robot confirmed the predictions concerning the consumed
	energy and stability. © 2002 Elsevier Science B.V. All rights reserved.},
  comment = {Cited By (since 1996): 6
	
	Export Date: 29 September 2006
	
	Source: Scopus
	
	CODEN: RASOE
	
	doi: 10.1016/S0921-8890(01)00177-4},
  file = {C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Capi02Optimal.pdf:C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Capi02Optimal.pdf:PDF},
  issn = {09218890 (ISSN)},
  keywords = {Biped robots, Genetic algorithm, Minimum energy, ZMP, Energy utilization,
	Genetic algorithms, Optimization, Torque, Trajectories, Biped robots,
	Trajectory planning methods, Robots},
  owner = {Vicepresideencia},
  review = {El trabajo de Capi et al. trata el problema de obtener una caminata
	estable y con un consumo energético óptimo para un robot bípedo de
	articulaciones prismáticas. El lazo de control se basa en el principio
	de Punto de Momento Cero (ZMP), el cual recibe una referencia de
	ubicación del centro de gravedad del robot, genera una señal de control
	que recibe, simultáneamente con la ubicación del ZMP, el lazo de
	ZMP. Se usa un algoritmo genético con codificación real (equivalente
	a una estrategia evolutiva) para buscar la trayectoria de referencia
	que genera el consumo mínimo de energía. Se comparan los resultados
	con la optimización para mínimo torque aplicado y para altura de
	centro de gravedad constante. El problema de optimización con restricciones
	es convertido a un problema sin restricciones al cual se le añaden
	penalizaciones en la función de aptitud cuando las condiciones de
	restricción son incumplidas.
	
	
	Este trabajo es innovativo al tratar el control de un robot caminador
	con articulaciones prismáticas. Es interesante la aproximación dada
	al problema de minimización del consumo de enrgía, optimizando la
	trayectoria de referencia. Es importante confrontar estos métodos
