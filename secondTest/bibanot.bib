@ARTICLE{Benbrahim97Biped,
  author = {Benbrahim, H. and Franklin, J.A.},
  title = {Biped dynamic walking using reinforcement learning},
  journal = {Robotics and Autonomous Systems},
  year = {1997},
  volume = {22},
  pages = {283--302},
  number = {3-4},
  __markedentry = {[Juan Figueredo]},
  abstract = {This paper presents some results from a study of biped dynamic walking
	using reinforcement learning. During this study a hardware biped
	robot was built, a new reinforcement learning algorithm as well as
	a new learning architecture were developed. The biped learned dynamic
	walking without any previous knowledge about its dynamic model. The
	self scaling reinforcement (SSR) learning algorithm was developed
	in order to deal with the problem of reinforcement learning in continuous
	action domains. The learning architecture was developed in order
	to solve complex control problems. It uses different modules that
	consist of simple controllers and small neural networks. The architecture
	allows for easy incorporation of new modules that represent new knowledge,
	or new requirements for the desired task.},
  comment = {Cited By (since 1996): 20
	
	Export Date: 14 September 2006
	
	Source: Scopus
	
	CODEN: RASOE},
  file = {C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Benbrahim1997.pdf:C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Benbrahim1997.pdf:PDF},
  issn = {09218890 (ISSN)},
  keywords = {Biped robot, Biped walking, Legged robot, Reinforcement learning,
	Robot learning, Control theory, Dynamics, Learning algorithms, Neural
	networks, Hardware biped robot, Legged robot, Self scaling reinforcement
	learning algorithm, Robot learning},
  owner = {Vicepresideencia},
  review = {Benbrahim y Franklin desarrollan un Generador Central de Patrones
	(CPG) utilizando redes neuronales CMAC y aplicando simultáneamente
	aprendizaje por refuerzo y supervisado. Para ello, se usa una red
	central, el generador, apoyada por un conjunto de redes periféricas
	de control acompañadas paralelamente por redes de observación, las
	cuales tienen como entrada parámetros relevantes que actúan como
	restricciones a la caminata, entre ellos postura corporal y altura
	corporal. La redes de control periféricas sólo actúan si su correspondiente
	red de observación detecta que la red central se está desmpeñando
	incorrectamente para una restricción específica. La salida del sistema
	de control será determinada por una función gaussiana con media ubicada
	en la salida del CPG y una desviación estandar que inicia en un nivel
	elevado y se reduce a medida que se llega a una solución convergente.
	El control dinámico necesario para seguir la referencia provista
	por el CPG es implementado con un conjunto de controladores PID.
	En la aplicación real, el esquema de control fue apoyado por un control
	de postura de encendido-apagado (on/off). Se requiere un entrenamiento
	previo para obtener tiempos adecuados de entrenamiento de las redes.
	
	
	
	Es bastante novedosa la implementación simultánea de aprendizaje por
	refuerzo y supervisado. Particularmente el acompañamiento de las
	redes de supervisión (críticas) permite obtener un satisfactorio
	aprendizaje dinámico, mientras las redes de apoyo evitan que un error
	del generador cause una falla general en la caminata. Sería conveniente
	trabajar en un control más óptimo y robusto de seguimiento que el
	PID implementado, así como una técnica para eliminar el control de
	postura on/off de apoyo. [Destacado] [CPG] [Neuronal]},
  timestamp = {2006.09.14}
}

@ARTICLE{Bergener99Complex,
  author = {Bergener, T. and Bruckhoff, C. and Dahm, P. and Janssen, H. and Joublin,
	F. and Menzner, R. and Steinhage, A. and Von Seelen, W.},
  title = {Complex behavior by means of dynamical systems for an anthropomorphic
	robot},
  journal = {Neural Networks},
  year = {1999},
  volume = {12},
  pages = {1087--1099},
  number = {7-8},
  __markedentry = {[Juan Figueredo]},
  abstract = {We present an architecture to generate behavior for an anthropomorphic
	robot. The goal is to equip the robot with the capacity to interact
	with a human. Motivated by the research on biological systems, our
	basic assumption is that the behavior to perform determines the external
	and internal structure of the behaving system. We describe the anthropomorphic
	design of our robot and present a distributed control system that
	generates human-like navigation and manipulation behavior. As the
	mathematical framework for this purpose we have developed a control
	system which is entirely based on dynamical systems in the form of
	instantiated dynamics and neural fields. We also present a dynamic
	scheme for the behavioral organization based on competitive dynamics.
	Copyright (C) 1999 Elsevier Science Ltd.},
  comment = {Cited By (since 1996): 7
	
	Export Date: 25 September 2006
	
	Source: Scopus
	
	CODEN: NNETE},
  file = {C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Bergener1999.pdf:C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Bergener1999.pdf:PDF},
  issn = {08936080 (ISSN)},
  keywords = {Anthropomorphic robot, Behavior organization, Dynamic approach, Robot
	human interaction, Anthropomorphic robots, Control system synthesis,
	Distributed parameter control systems, Dynamics, Human computer interaction,
	Mathematical models, Motion control, Behavior organization, Dynamic
	approach, Robot human interaction, Neural networks, behavior, computer
	aided design, robotics},
  owner = {Vicepresideencia},
  publisher = {Elsevier Science Ltd},
  review = {El trabajo de Bergener et al. describe una arquitectura que permite
	generar tanto patrones de comportamiento como controlar dinámicamente
	la ejecución de cada tarea. A diferencia de otros trabajos evaluados,
	la arquitectura se le aplica a un robot antropomórfico que no es
	bípedo y cuya similitud con la morfología humana se observa principalmente
	en su brazo, y por tanto los problemas abordados no incluyen la caminata.
	La arquitectura presentada emplea campos neuronales (neural fields)
	que realizan el mapeo desde los sensores hacia los actudores utilizando
	principios de sistemas dinámicos cuyo comportamiento se adapta a
	las condiciones del entorno a través de fenómenos de bifurcación
	que responden a lo que los autores llaman dinámica instanciada (un
	comportamiento elemental parametrizado por variables de comportamiento
	en un ambiente específico). Así mismo, para generar comportamientos
	complejos se usa un mecanismo de arbitramiento se usa un sistema
	dinámico competitivo, el cual icluye un conjunto de parámetros que
	describe los requerimientos lógicos y temporales.
	
	
	Su aplicación a robótica evolutiva humanoide consiste en prensión
	de objetos con el brazo y seguimiento de comportamiento de referencia,
	y se presenta adicionalmente una aplicación interesante a la navegación
	con obstáculos (posiblemente dinámicos), si bien el sistema es impulsado
	con ruedas. Es destable la perspectiva abordada desde los sistemas
	dinámicos no-lineales. Permite extender la conceptualización a problemas
	de navegación y control en la caminata bípeda. [Destacado] [Fuera
	de Tema Enfocado] [Dinámico][Neuronal]},
  timestamp = {2006.09.25}
}

@ARTICLE{Buzzi03Nonlinear,
  author = {Buzzi, U.H. and Stergiou, N. and Kurz, M.J. and Hageman, P.A. and
	Heidel, J.},
  title = {Nonlinear dynamics indicates aging affects variability during gait},
  journal = {Clinical Biomechanics},
  year = {2003},
  volume = {18},
  pages = {435--443},
  number = {5},
  abstract = {Objective. To investigate the nature of variability present in time
	series generated from gait parameters of two different age groups
	via a nonlinear analysis. Design. Measures of nonlinear dynamics
	were used to compare kinematic parameters between elderly and young
	females. Background. Aging may lead to changes in motor variability
	during walking, which may explain the large incidence of falls in
	the elderly. Methods. Twenty females, 10 younger (20-37 yr) and 10
	older (71-79 yr) walked on a treadmill for 30 consecutive gait cycles.
	Time series from selected kinematic parameters of the right lower
	extremity were analyzed using nonlinear dynamics. The largest Lyapunov
	exponent and the correlation dimension of all time series, and the
	largest Lyapunov exponent of the original time series surrogated
	were calculated. Standard deviations and coefficient of variations
	were also calculated for selected discrete points from each gait
	cycle. Independent t-tests were used for statistical comparisons.
	Results. The Lyapunov exponents were found to be significantly different
	from their surrogate counterparts. This indicates that the fluctuations
	observed in the time series may reflect deterministic processes by
	the neuromuscular system. The elderly exhibited significantly larger
	Lyapunov exponents and correlation dimensions for all parameters
	evaluated indicating local instability. The linear measures indicated
	that the elderly demonstrated significantly higher variability. Conclusions.
	The nonlinear analysis revealed that fluctuations in the time series
	of certain gait parameters are not random but display a deterministic
	behavior. This behavior may degrade with physiologic aging resulting
	in local instability. Relevance: Elderly show increased local instability
	or inability to compensate to the natural stride-to-stride variations
	present during locomotion. We hypothesized that this may be the one
	of the reasons for the increases in falling due to aging. Future
	efforts should attempt to evaluate this hypothesis by making comparisons
	to pathological subjects (i.e. elderly fallers), and examine the
	sensitivity and specificity of the nonlinear methods used in this
	study to aid clinical assessment. © 2003 Elsevier Science Ltd. All
	rights reserved.},
  address = {Department of Mathematics, University of Nebraska at Omaha, Omaha,
	NE 68182, United States},
  comment = {Cited By (since 1996): 13
	
	Export Date: 30 October 2006
	
	Source: Scopus},
  keywords = {Chaos, Elderly, Locomotion, Nonlinear dynamics, Variability},
  owner = {Juan Figueredo},
  timestamp = {2006.10.30}
}

