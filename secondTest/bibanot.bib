@ARTICLE{Duysens02walking,
  author = {Duysens, J. and Van de Crommert, H.W.A.A. and Smits-Engelsman, B.C.M.
	and Van der Helm, F.C.T.},
  title = {A walking robot called human: Lessons to be learned from neural control
	of locomotion},
  journal = {Journal of Biomechanics},
  year = {2002},
  volume = {35},
  pages = {447--453},
  number = {4},
  __markedentry = {[Juan Figueredo]},
  abstract = {From what we know at present with respect to the neural control of
	walking, it can be concluded that an optimal biologically inspired
	robot could have the following features. The limbs should include
	several joints in which position changes can be obtained by actuators
	across the joints. The control of mono- and biarticular actuators
	should occur at least at three levels: one at direct control of the
	actuators (equivalent to motoneuron level), the second at indirect
	control acting at a level which controls whole limb movement (flexion
	or extension) and the third at a still higher level controlling the
	interlimb coordination. The limb level circuits should be able to
	produce alternating flexion and extension movements in the limb by
	means of coupled oscillator flexor and extensor parts which are mutually
	inhibitory. The interlimb control level should be able to command
	the various limb control centers. All three control levels should
	have some basic feedback circuits but the most essential one is needed
	at the limb control level and concerns the decision to either flex
	or extend a given limb. The decision to activate the extensor part
	of the limb oscillator has to be based on feedback signalling the
	onset of loading of the limb involved. This should be signalled by
	means of load sensors in the limb. The decision to activate the flexor
	part of the limb oscillator has to depend on various types of feedback.
	The most important requirement is that flexion should only occur
	when the limb concerned is no longer loaded above a given threshold.
	The rule for the initiation of limb flexion can be made more robust
	by adding the requirement that position at the base of the limb ("hip")
	should be within a normal end of stance phase range. Hence, human
	locomotion is thought to use a number of principles which simplify
	control, just as in other species such as the cat. It is suggested
	that cat and human locomotion are good models to learn from when
	designing efficient walking robots. © 2002 Elsevier Science Ltd.
	All rights reserved.},
  comment = {Cited By (since 1996): 1
	
	Export Date: 9 October 2006
	
	Source: Scopus
	
	CODEN: JBMCB
	
	doi: 10.1016/S0021-9290(01)00187-7},
  file = {C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Duysens00walking.pdf:C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Duysens00walking.pdf:PDF},
  issn = {00219290 (ISSN)},
  keywords = {Exteroceptive reflexes, Human gait, Phase-dependent reflex modulation,
	Proprioceptive reflexes, Human engineering, Load limits, Neurology,
	Robots, Biarticular actuators, Whole limb movements, Biped locomotion,
	locomotion, neuromodulation, robotics},
  owner = {Juan Figueredo},
  review = {Duysens et al. hacen una revisión biológica al control neuronal de
	la caminata en seres humanos y gatos, la cual proponen como base
	para desarrollos en robótica articulada bípeda o cuadrúpeda. Se tratan
	dos puntos fundamentales. En una menor extensión, se menciona a grandes
	rasgos la arquitectura neuronal en capas, con una capa de bajo nivel
	de motoneuronas de flexión/extensión acopladas inhibitoriamente,
	una capa media de interneuronas generadoras de patrones, y una capa
	alta de control y cordinación de movimiento global. Con mayor detalle
	los autores destacan el efecto de las entradas sensoriales y sus
	respuestas por reflejo en la acción de las diferentes capas de la
	arquitectura neuronal. A bajo nivel muestran diferentes resultados
	previos que indican la posible participación de respuestas por reflejo
	en la intensidad de las señales de las motoneuronas, modificando
	las curvas de acceleración del movimiento. A nivel medio, destacan
	la importancia de las entradas sensoriales para mantener un estímulo
	en la extensión de una pierna cuando ésta soporta carga, así como
	permitir la flexión cuando no la soporta y estimularla cuando hay
	una flexión excesiva. Finalmente concluyen que a pesar del poco conocimiento
	sobre la capa superior de control, el conocimiento arrojado por estudios
	sobre las dos capas inferiores es suficientemente valioso como para
	se aplicado a la robótica.
	
	
	Este es un trabajo que sirve como reseña biológica para la comunidad
	que trabaja en el área de robótica bípeda, y es de especial interés
	para aquellos interesados en el control neuronal, particularmente
	por el uso de generadores de patrones. Se destaca la argumentación
	a favor de la respuesta por reflejo y la útilidad de las señales
	sensóricas, yendo en sentido contrario a algunos de los trabajos
	reseñados, que argumentan la capacidad de un generador de patrones
	de producir marcha sin realimentación sensorial. [Intersante] [Muestra
	principios biológicos más no diseño de controladores] [Especial]
	[Neuronal]},
  timestamp = {2006.10.09}
}

