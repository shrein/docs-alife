@INPROCEEDINGS{Lee07Reaction,
  author = {Sung-hee Lee and Ambarish Goswami},
  title = {Reaction mass pendulum (RMP): An explicit model for centroidal angular
	momentum of humanoid robots},
  booktitle = {in Proceedings of the IEEE International Conference on Robotics \&
	Automation},
  year = {2007},
  __markedentry = {[jjfigueredou]},
  abstract = {A number of conceptually simple but behaviorrich “inverted pendulum”
	humanoid models have greatly enhanced the understanding and analytical
	insight of humanoid dynamics. However, these models do not incorporate
	the robot’s angular momentum properties, a critical component of
	its dynamics. We introduce the Reaction Mass Pendulum (RMP) model,
	a 3D generalization of the better-known reaction wheel pendulum.
	The RMP model augments the existing models by compactly capturing
	the robot’s centroidal momenta through its composite rigid body (CRB)
	inertia. This model provides additional analytical insights into
	legged robot dynamics, especially for motions involving dominant
	rotation, and leads to a simpler class of control laws. In this paper
	we show how a humanoid robot of general geometry and dynamics can
	be mapped into its equivalent RMP model. A movement is subsequently
	mapped to the time evolution of the RMP. We also show how an “inertia
	shaping” control law can be designed based on the RMP.}
}

@ARTICLE{Liu03Hybrid,
  author = {Liu, Z. and Li, C. and Xu, W.},
  title = {Hybrid control of biped robots in the double-support phase via H
	? approach and fuzzy neural networks},
  journal = {IEE Proceedings: Control Theory and Applications},
  year = {2003},
  volume = {150},
  pages = {347--354},
  number = {4},
  __markedentry = {[Juan Figueredo]},
  abstract = {A quadratic stabilisation fuzzy neural network hybrid control scheme
	is proposed for biped robots in the double-support phase. First,
	the authors considered the holonomic constraints. The walking locomotion
	of biped robots in the double-support phase is modelled as a reduced
	order position/force hybrid model, which integrates the position/force
	hybrid model with the reduced-order model. Then a new fuzzy neural
	network hybrid control scheme is presented, in which H? approach,
	inverse system method and variable structure control are used to
	attenuate the effect of external disturbances and parametric uncertainties.
	Simulation results are reported in order to show the performance
	of the proposed control scheme.},
  comment = {Cited By (since 1996): 2
	
	Export Date: 14 September 2006
	
	Source: Scopus
	
	CODEN: ICTAE
	
	doi: 10.1049/ip-cta:20030631},
  file = {C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Liu03Hybrid.pdf:C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Liu03Hybrid.pdf:PDF},
  issn = {13502379 (ISSN)},
  keywords = {Computer simulation, Constraint theory, Fuzzy sets, Neural networks,
	Robots, Biped robots, Hybrid controls, Biped locomotion},
  owner = {Vicepresideencia},
  review = {El algoritmo híbrido propuesto por Liu et al. implementa un controlador
	para la fase de doble apoyo en la caminata bípeda divido en tres
	componentes. La primera utiliza una red neuronal CMAC para la cual
	se generan los conjuntos difusos, identificados como conjuntos de
	generalización de la red, cons los cuales son mapeadas las variables
	de entrada a un espacio de generalización, a partir del cual se obtiene
	la salida como una combinación lineal entre la representación generalizada
	para el conjunto de entradas y la matriz de pesos de la red neuronal
	difusa (Fuzzy Neural Network) de tipo CMAC. Posteriormente se deriva
	un modelo híbrido posición/fuerza para el sistema incluyendo restricciones,
	con el cual posteriormente se haya un modelo para el sistema híbrido
	aplicando un esquema de control que incluye el uso de la FNN. Se
	implementa un parámetro de optimización H-infinito para el modelo
	conjunto, el cual se optimiza. Se obtiene finalmente un controlador
	híbrido robusto el cual se vale de la FNN para el control del sistema
	inverso y de control de estructura variable. Se obtiene así un método
	robusto para seguimiento de una trayectoria deseada en doble apoyo.
	Se sugiere el estudio de tecnicas de teoría de sistemas interruptores
	(switching system theory) para, acoplándolas con la técnica de control
	propuesta, controlar integradamente las diferentes fases de la marcha.
	
	
	El artículo es valioso en cuanto aborda el problema de control en
	la fase de doble apoyo no suficientemente tratado, y propone un sistema
	híbrido de control donde se articulan métodos de inteligencia computaiconal
	(una red neuronal difusa implementada como una red CMAC) con técnicas
	de control robusto (control H-infinito). En general presenta un plantamento
	matemático complejo, en el cual se representan adecuadamente las
	restricciones del sistema integrándolas en un modelo híbrido del
	sistema. Sin embargo, esto causa una dificil lectura del artículo
	y se requiere un conocimiento amplio de las bases teorícas de las
	que se fundamenta. [Interesante] [Alta complejidad -> Ha de ser releido
	a la luz de una conceptualización más profunda] [Dinámico][Neuronal][Difuso]},
  timestamp = {2006.09.28}
}

@ARTICLE{Magdalena97fuzzy,
  author = {Magdalena, L. and Monasterio-Huelin, F.},
  title = {A fuzzy logic controller with learning through the evolution of its
	knowledge base},
  journal = {Int J Approximate Reasoning},
  year = {1997},
  volume = {16},
  pages = {335--358},
  number = {3-4 SPEC. ISS.},
  __markedentry = {[Juan Figueredo]},
  abstract = {Fuzzy logic controllers (FLCs) constitute knowledge-based systems
	that include fuzzy rules and fuzzy membership functions to incorporate
	human knowledge into their knowledge base. The specification of fuzzy
	rules and fuzzy membership functions is one of the key question when
	designing FLCs, and is generally affected by subjective decisions.
	Some efforts have been made to obtain an improvement on system performance
	by incorporating learning mechanisms to modify the rules and/or membership
	functions of the FLC. Genetic algorithms are probabilistic search
	and optimization procedures based on natural genetics. This paper
	proposes a way to apply (with a learning purpose) genetic algorithms
	to FLCs, and presents an application designed to control the synthesis
	of the biped walk of a simulated 2-D biped robot. © 1997 Elsevier
	Science Inc.},
  comment = {Cited By (since 1996): 31
	
	Export Date: 29 September 2006
	
	Source: Scopus
	
	CODEN: IJARE},
  file = {C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Magdalena97fuzzy.pdf:C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Magdalena97fuzzy.pdf:PDF},
  issn = {0888613X (ISSN)},
  keywords = {Fuzzy logic control, Genetic algorithms learning, Genetic fuzzy systems,
	Computer simulation, Control system synthesis, Fuzzy sets, Genetic
	algorithms, Knowledge based systems, Learning systems, Membership
	functions, Optimization, Performance, Probability, Robots, Fuzzy
	logic controllers, Genetic fuzzy systems, Probabilistic search, Fuzzy
	control},
  owner = {Vicepresideencia},
  review = {El artículo de Magdalena y Monasterio-Huelin trata la evolución por
	algoritmos genéticos de un controlador por lógica difusa y presenta
	su aplicación a la generación de patrones de marcha en un caminador
	bípedo. Particularmente, se usa un algoritmo genético con codificacación
	binaria para evolucionar tanto las reglas difusas como los rangos
	de normalización para cada una de las variables involucradas, si
	bien se codifica toda la información relevante a la base de conocimiento,
	entre ella el número de conjuntos difusos por variable de entrada
	y de salida y la definición de las funciones de pertenencia. Se aplica
	cruce entre reglas difusas, mutación a nivel de bit de las reglas
	y mutación de los rangos de normalización. Se utiliza la metodología
	expusta para diseñar un controlador difuso para un caminador bípedo,
	el cual considera las diferentes fases de la marcha. Se obtiene como
	resultado no solo un conjunto de caminatas exitosas, sino también
	reglas con mejor desempeño que las provistas inicialmente al sistema.
	
	
	
	La metodología presentada por los autores permite desarrollar de manera
	sencilla controladores satisfactorios que pueden ser aplicados al
	problema de la marcha. Algunos de los procedimientos empleados podrían
	ser mejorados a la luz de desarrollos teóricos y experiencias de
	aplicaciones posteriores a 1997, pero en general sigue siendo válido
	el planteamiento. Sin embargo, la aproximación puede estar limitada
	en la complejidad de los problemas que puede abordar, en la rigueza
	de los comportamientos que puede generar y en la robustez de los
	controladores a cambios en el sistema o el entorno. [Dinámico][Difuso]},
  timestamp = {2006.09.28}
}

