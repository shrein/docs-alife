@INPROCEEDINGS{Komatsu05Dynamic,
  author = {Komatsu, T. and Usui, M.},
  title = {Dynamic walking and running of a bipedal robot using hybrid central
	pattern generator method},
  booktitle = {IEEE International Conference on Mechatronics and Automation, ICMA
	2005},
  year = {2005},
  pages = {987--992},
  __markedentry = {[Juan Figueredo]},
  abstract = {This paper presents simulation and experimental results of dynamic
	walking and running of a bipedal robot. We proposed the hybrid central
	pattern generator(H-CPG) method to realize adaptive dynamic motions
	including stepping and jumping. This method basically consisted of
	CPG models, but was added the force control system that controlled
	the acting force from a leg to a floor in the vertical and the horizontal
	directions separately. At this moment, 2D walking and running was
	realized on level ground and a slope with a speed of 1.6 m/s. © 2005
	IEEE.},
  comment = {Export Date: 25 September 2006
	
	Source: Scopus},
  file = {C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Dynamic walking and running of a bipedal robot using hybrid central pattern generator method.pdf:C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Dynamic walking and running of a bipedal robot using hybrid central pattern generator method.pdf:PDF},
  issn = {0780390458 (ISBN)},
  keywords = {Bipedal robot, Control, Humanoid, Neural oscillator},
  owner = {Juan Figueredo},
  review = {Komatsu y Usul muestran el control para diferentes tipos de locomoción
	bípeda usando generadores centrales de patrones híbridos (H-CPG).
	La acción total del H-CPG está determinada por la suma de las acciones
	individuales de cada uno de sus componentes: Un oscilador neuronal
	que genera los patrones rítmicos en forma de torques, un controlador
	de fuerza contra el suelo que aplica la matriz jacobiana para mapear
	las fuerzas del espacio cartesiano al espacio de articulaciones,
	y un controlador de postura que realiza un control PD para mantener
	las piernas lo más verticales posible. Los movimientos verticales
	y horizontales son procesados por separado por el controlador de
	fuerza. Muestran que el método propuesto permite, tanto como variar
	de un movimiento de caminata a uno de trote o de carrera, como caminar
	y correr en ambientes modificados, particularmente en pendientes.
	Se muestran resultados satisfactorios tanto en simulaciones como
	en una implementación sobre un robot real. 
	
	
	Son muy interesantes los resultados obtenidos en cuanto a la versatilidad
	de movimiento lograda al añadir un control de fuerza y de postura
	al ya típico control con CPGs. A pesar de que las redes son diseñadas
	para cada movimiento deseado, su complejidad no es muy alta y resulta
	factible. El trabajo puede ser complementado estudiando cómo integrar
	los controladores y el CPG en una sola arquitectura que sea robusta
	y adaptable, lo cual incluye emplear posiblemente métodos de aprendizaje
	o evolución, de tal forma que el sistema puede ser autónomo. [Destacado]
	[CPG] [Neuronal]},
  timestamp = {2006.10.23}
}

