@MISC{Kun97Adaptive,
  author = {A. Kun and W. Miller},
  title = {Adaptive Static Balance of a Biped Robot Using Neural Networks},
  year = {1997},
  __markedentry = {[Juan Figueredo]},
  abstract = {This paper describes the static walking control strategy designed
	and implemented on Toddler, the UNH biped, in order to test the validity
	of the above strategies. Early bipeds walked with static balance,
	usually with large feet and slow walking speeds. Summary discussions
	of the early history of biped walking machines have been presented
	by Raibert [1] and others. A recent implementation of static biped
	walking is the work of Zheng and Shen in which they devised, and
	experimentally verified, a ...},
  file = {C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Kun97Adaptive.pdf:C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Kun97Adaptive.pdf:PDF},
  owner = {Juan Figueredo},
  review = {Kun y Miller tratan de nuevo el problema de la caminata bípeda, pero
	ahora en equilibrio estático. A diferencia de [Kun96Adaptive], aquí
	se busca la locomoción bípeda realizando el movimiento cuasi-estáticamente
	y por tanto el criterio de estabilidad requiere que la proyección
	del centro de masa se encuentre dentro del polígono de sustentación
	en todo momento, pudiendo así permanecer el cualquier posición indefinidamente.
	La generación de trayectorias, como en el otro trabajo anotado, es
	realizada con redes neuronales CMAC, partiendo de trayectorias pre-calculadas
	pero adaptables. En este caso se usa una red para la elevación del
	pié, una para el movimiento frontal y otra para el movimiento lateral.
	Como el modelo de la cinemática usado es aproximado, se usan otras
	dos redes CMAC para realizar correciones al patron de marcha de tal
	forma que se corrigan las incosistencias del modelo. Se acompaña
	al generador con un control de bajo nivel que se encarga de convertir
	la postura generada al espacio de articulaciones, hacer el seguimiento
	de la trayectoria, mantener la posición perpendicular al suelo en
	la fase de doble apoyo y realizan un control reactivo lateral y frontal.
	Se logra una marcha satisfactoria en un robot de 10-DOF con una velocidad
	de 2.2 pasos por minuto.
	
	
	Aunque la caminata estática se ha venido dejando relegada en favor
	de la caminata dinámica, es un escenario interesante de control en
	los camindores bípedos y puede resultar relevante en situaciones
	determinadas. Es destacable que el método de solución propuesto se
	asemeja bastante al mostrado para la caminata dinámica en [Kun96Adaptive],
	sugiriendo interesante posibilidad de integrar los dos controles
	en uno solo. [Interesante -> Posibilidad de integración de caminata
	estática y dinámica] [Estático] [CPG] [Neuronal]},
  text = {Kun A. L., Miller W. T. (1997), Adaptive Static Balance of a Biped
	Robot Using Neural Networks, Proc. of the Int.},
  timestamp = {2006.10.08},
  url = {citeseer.ist.psu.edu/kun97adaptive.html}
}

@MISC{Kun96Adaptive,
