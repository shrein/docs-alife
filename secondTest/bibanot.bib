}

@MISC{Vaughan03Flight,
  author = {Eric Vaughan},
  title = {Flight of the Biped},
  howpublished = {Simulation of Adaptive Behavior, University of Sussex},
  year = {2003},
  abstract = {Despite technological advances in wheeled vehicles over the last 100
	years legs are still
	
	the most efficient form of land locomotion known. 50% of land on earth
	is inaccessible to
	
	wheeled or tracked vehicles. In these more difficult conditions, wheeled
	vehicles can only
	
	achieve 3-5 mph, while animals reach speeds of up to 35 mph [Ano67].
	Legs can increase
	
	traction and decrease energy consumption, by taking advantage of strategic
	footholds in
	
	the terrain [Rai86]. In 10-inch plastic soil wheeled vehicles require
	15hp/ton, while legs
	
	only require 7hp/ton. Vehicles modeled after humanoid locomotion,
	could in theory
	
	travel efficiently upstairs and through urban terrain. This could
	allow them to assist in
	
	dangerous environments, such as firefighting and bomb disposal. To
	harness these
	
	properties, extensive research has been done in bipedal walking. The
	most famous are the
	
	Honda Humanoids: P2, P3, and Asimo [Hir98]. These machines are the
	result of 10
	
	years of active research, and are capable of walking on sloped surfaces
	and climbing
	
	stairs. However, they don’t appear to have many of the attributes
	advertised by legged
	
	locomotion. They are very inefficient at walking, requiring over 20
	times the muscle
	
	work rate, of an equally sized person [Col01]. And they are incapable
	of traveling at
	
	speeds faster than a slow walk. These deficiencies may be a result
	of the approach taken
	
	in their design. By studying the walking gait of humans, mathematical
	models were
	
	created to mimic the trajectories and motion of their legs. However,
	this is analogous to
	
	modeling the flapping motions of a bird’s wings, in order to understand
	flight. Building a
	
	model of a bird and mechanically playing back wing gaits was not instrumental
	in the
	
	Wright bothers approach to flight [Hep03]. Instead they studied the
	interaction between a
	
	bird’s wings and the air, to understand the dynamics of lift. With
	this knowledge they
	
	built an un-powered glider, and later added a small amount of propulsion.
	Over time this
	
	evolved into streamlined planes in which the shape and engine were
	perfectly matched.
	
	This paper theorizes that in order to realize human locomotion, a
	similar approach needs
	
	to be taken. First the equivalent of an un-powered biped walker should
	be constructed
	
	and perfected. Then the control system should be co-evolved with the
	biped’s
	
	morphology to achieve fast, efficient, natural walking.},
  file = {C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Vaughan03Flight.pdf:C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Vaughan03Flight.pdf:PDF},
  owner = {Juan Figueredo},
  review = {Vaughan realiza una revisión general de algunos de los métodos para
	el control de caminata bípeda. Primero, hace una referencia a los
	métodos de seguimiento de trayectorias predefinidas que fueron usados
