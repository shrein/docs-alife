@INPROCEEDINGS{Pasemann98Evolving,
  author = {Frank Pasemann},
  title = {Evolving neurocontrollers for balancing an inverted pendulum},
  booktitle = {Network: Computation in Neural Systems},
  year = {1998},
  pages = {495--511},
  __markedentry = {[jjfigueredou]},
  abstract = {The paper introduces an evolutionary algorithm that is tailored to
	generate recurrent neural networks functioning as nonlinear controllers.
	Network size and architecture as well as network parameters like
	weights and bias terms are developed simultaneously. There is no
	quantization of inputs, outputs or internal parameters. Dierent kinds
	of evolved networks are presented that solve the pole-balancing problem,
	i.e. balancing an inverted pendulum. Especially controllers solving
	the problem for reduced phase space information (only angle and cart
	position) use a recurrent connectivity structure. Evolved controllers
	of \minimal &quot; size still have a very good benchmark performance.}
}

@MISC{Paul04Sensorimotor,
  author = {Chandana Paul},
  title = {Sensorimotor Control of Biped Locomotion based on Contact Information},
  year = {2004},
  __markedentry = {[Juan Figueredo]},
  abstract = {Human locomotion is extremely adaptive and robust to changes in environment,
	body size and morphology. The control of this highly adaptive system
	is thought to be performed by central pattern generators, supported
	by a system of sensorimotor connections called reflexes. Several
	studies on central pattern generators have been conducted, alone
	and in concert with reflexes. However, controllers based on direct
	sensorimotor reflexes have not been given much attention, as they
	have probably been considered too simple for the task of controlling
	a biped. In this paper, however, we present results to the contrary.
	It is shown that a purely sensorimotor neural network, that is one
	with direct connections between sensors and motors can produce stable
	gait in an 8 degree-of-freedom simulated lower body biped. It is
	shown that sensorimotor control of biped walking can be accomplished
	using a network with only two cutaneous inputs and four neurons.
	The results suggest that reflexes may play a large role in the coordination
	of movement. They also lead to new insight into the integration of
	central movement commands with adaptive reflexes using the dynamics
	of the body.},
  file = {C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Paul04Sensorimotor.pdf:C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Paul04Sensorimotor.pdf:PDF},
  owner = {Juan Figueredo},
  review = {En el este artículo Paul busca continuar el trabajo orientado a la
	simplificación de controladores para la caminata basados en redes
	neuronales, esta vez mostrando que redes simples independientes para
	el control de cada pierna pueden generar un caminata estable, usando
	sólo conexiones hacia adelante (feed-fordward neural network) sin
	capa oculta. Las redes utilizadas sólo reciben como entrada el contacto
	con su respectiva pierna y una señal de bias común para los dos controladores.
	De numerosas configuraciones evolucionadas con algoritmos genéticos,
	sólo dos lograron la caminata satisfactoria, pero en forma destacable,
	ambas resultaron ser estables indefinidamente. Se usa, como en previos
	trabajos de Paul, coevolución de algunos pocos parámetros morfológicos.
	
	
	El trabajo es destacable en cuanto muestra una de las alternativas
	más simples para el control de la caminata bípeda, realizando un
	simple mapeo entre la entrada sensórica de cada pierna y su movimiento,
	adicionalmente exhibiento completo desacople contralateral. Esto
	lleva a Paul a conjeturar que el control bípedo en los humanos en
	las extremidades inferiores es efectuado enteramente por coordinación
	sensoriomotriz, mientras que el CPG se usa para alterar las condiciones
	de la marcha actuando sobre el tronco y brazos. Tal conjetura ha
	de ser probada más cuidadosamente. [CPG]},
  timestamp = {2006.10.08}
}

@MISC{Paul03Bilateral,
  author = {Chandana Paul},
  title = {Bilateral Decoupling in the Neural Control of Biped Locomotion},
  year = {2003},
  __markedentry = {[Juan Figueredo]},
  abstract = {In this paper, a bilaterally decoupled neural controller was used
	to achieve stable locomotion in a 6 DOF biped robot with 5 links,
	implemented in a virtual physics-based simulation environment.},
  file = {C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Chandana03Bilateral.pdf:C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Chandana03Bilateral.pdf:PDF},
  owner = {Juan Figueredo},
  review = {En este trabajo Paul busca mostrar que, a diferencia de los controladores
	que usan un generador central de patrones (CPG) cuyas conexiones
	entre los osciladores en los lados izquierdo y derecho están acoplados,
	es posible obtener una caminata satisfactoria con dos generadores
	desacoplados. Se tratan dos casos: el primero desacopla tanto las
	conexiones como la sensórica, teniéndose dos CPGs independientes,
	sin ninguna conexión entre sí y con entradas de sensores ubicados
	estríctamente en su lado parcial; mientras el segundo desacopla las
	conexiones pero se permiten entradas comunes a ambos sensores referentes
	a la postura global. Ambos controladores son evolucionados con algoritmos
	genéticos y en paralelo a tres parámetros morfológicos. Se muestra
	que para el primer caso se logran caminatas satisfactorias en la
	mayoría de los casos pero hay problemas para seguir la línea recta.
	En el segundo, se demuestra que el desacople de los CPGs con entradas
	globales es una técnica igualmente satisfactoria que el caso enteramente
	acoplado.
	
	
	Es interesante la prueba práctica que da el autor a favor de la posibilidad
	de que el control en la caminata bípeda humana se encuentre desacoplado
	lateralmente. Así mismo, se destaca que la estructura de control,
	así como los pocos parámetros morfológicos escogidos, resultaron
	en un conjunto de caminadores bastante exitosos y es un prograe considerable
	frente al trabajo previo [Paul01road]. [Especia] [CPG]},
  timestamp = {2006.10.08},
  url = {citeseer.ist.psu.edu/611438.html}
}

@MISC{Paul01road,
  author = {C. Paul and J. Bongard},
  title = {The road less travelled: Morphology in the optimization of biped
	robot locomotion},
  year = {2001},
  __markedentry = {[Juan Figueredo]},
  abstract = {In this paper, stable bipedal locomotion has been achieved using coupled
	evolution of morphology and control on a 5-link biped robot in a
	physics-based simulation environment. The robot was controlled by
	a closed loop recurrent neural network controller. The goal was to
	study the effect of macroscopic, midrange and microscopic changes
	in mass distribution along the biped skeleton to ascertain whether
	optimal morphology and control pairs could be discovered.
	
	The sensor-motor coupling determined that small changes in morphology
	manifest themselves as large changes in the performance of the biped,
	which were exploited by the optimization process. In this way, mechanical
	design and controller optimization were reduced to a single process,
	and more mutually optimized designs resulted. This work points to
	alternative routes for efficient automated and manual biped optimization.},
  file = {C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Paul01road.pdf:C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Paul01road.pdf:PDF},
  owner = {Juan Figueredo},
  review = {En el trabajo de Paul y Bongard se propone novedosamente la evolución
	acoplada de morfología y control, destacándose la relativa poca profundidad
	con la que se ha tratado la evolución (y en general la configuración)
	morfológica para la obtención de patrones de marcha. La metodología
	utilizada consiste en evolucionar simultáneamente la morfología,
	en términos de la distribución de bloques discretos de masa en una
	configuración articular fija, y el control, en términos de los pesos
	de una red neuronal recurrente. Se realizan varios experimentos,
	variando la proporción de masa que puede ser redistribuida, con el
	fin de evaluar las modificaciones morfológicas en escalas micro,
	meso y macro.
	
	
	Es muy importante recuperar la temática de estudio del trabajo como
	una de las áreas fundamentales en el diseño de robots bípedos en
	la cual la inteligencia computacional puede jugar un papel importante
	(ver también el trabajo de [Huelse04Structure]). Hay múltiples elementos
	en los cuales se puede trabajar para optimizar el proceso, entre
	ellos los operadores y la representación en el algoritmo evolutivo
	(pueden estar restringiendo el espacio de búsqueda o la movilidad
	a través de este), así como en las técnicas de control. [Interesante
	-> Destaca la importancia de modificación morfológica paralela al
	control] [Dinámico][Neuronal]},
  text = {Chandana Paul and Josh C. Bongard. The road less travelled: Morphology
	in the optimization of biped robot locomotion. In Proceedings of
	the IEEE/RSJ International Conference on Intelligent Robots and Systems,
	volume 1, pages 226--232, Piscataway, NJ, 2001. IEEE Press.},
  timestamp = {2006.10.08},
  url = {citeseer.ist.psu.edu/article/paul01road.html}
}

@ARTICLE{Reil02Evolution,
  author = {Reil, T. and Husbands, P.},
  title = {Evolution of central pattern generators for bipedal walking in a
	real-time physics environment},
  journal = {IEEE Trans Evol Comput},
  year = {2002},
  volume = {6},
  pages = {159--168},
  number = {2},
  __markedentry = {[Juan Figueredo]},
  abstract = {We describe an evolutionary approach to the control problem of bipedal
	walking. Using a full rigid-body simulation of a biped, it was possible
	to evolve recurrent neural networks that controlled stable straight-line
	walking on a planar surface. No proprioceptive information was necessary
	to achieve this task. Furthermore, simple sensory input to locate
	a sound source was integrated to achieve directional walking. To
	our knowledge, this is the first work that demonstrates the application
	of evolutionary optimization to three-dimensional physically simulated
	biped locomotion.},
  comment = {Cited By (since 1996): 11
	
	Export Date: 29 September 2006
	
	Source: Scopus
	
	CODEN: ITEVF
	
	doi: 10.1109/4235.996015},
  file = {C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Evolution of central pattern generators for bipedal walking in a real-time physics environment.pdf:C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Evolution of central pattern generators for bipedal walking in a real-time physics environment.pdf:PDF},
  issn = {1089778X (ISSN)},
  keywords = {Bipedal walking, Evolutionary algorithms, Evolutionary robotics, Physics,
	Recurrent neural networks, Biped locomotion, Evolutionary algorithms,
	Optimization, Recurrent neural networks, Pattern generators, Robotics},
  owner = {Juan Figueredo},
  review = {Reil y Husbands emplean un algoritmo evolutivo para optimizar los
	parámetros de una red neuronal recurrente totalmente conectada. El
	modelo de cada neurona es un modelo dinámico que atenúa su respuesta
	en ausencia de estimulación, y depende de un conjunto de pesos, un
	factor de sesgo (bias) y una constante de tiempo dinámica. Los tres
	tipos de parámetros para la totalidad de neuronas de la red son representados
	linealmente en el genotipo de los individuos del algoritmo evolutivo.
	La función de aptitud empleada favorece el movimiento que maximize
	el desplazamiento en dirección frontal y penaliza el movimiento en
	dirección vertical. Los autores logran con tal regla de evaluación
	sencilla generar patrones de marcha satisfactorios sin necesidad
	de ninguna realimentación sensorial. Posteriormente, adicionan una
	entrada sensorial de tipo 'auditivo' y, dejando fijas las conexiones
	recurrentes, evolucionan los pesos de conexión de las neuronas recurrentes
	con la entrada sensorial. Logran de esta forma que el robot navegue
	hacia una fuente de emisión de sonido. El controlador neuronal genera
	referencias de posición que son llevadas a fuerzas en los motores
	por un control PD de torque. Una adición a la eficiencia del algoritmo
	es el abandono prematura de marchas que se desplazan demaisado en
	dirección vertical.
	
	
	Este trabajo se puede considerar dentro del grupo de generadores centrales
	patrones, con la diferencia que aquí no se uitlizan osciladores acoplados,
	sino una red completamente realimentada. Sin embargo, los autores
	propones usar subredes acopladas en posteriores trabajos. Es importante
	mencionar dos puntos adicionales: logran caminata sin necesidad de
	una entrada sensorial y empleando una función de aptitud sencilla,
	y el método es extensible a situaciones con realimentación sensorial
	simple. Dado que sólo un 10% de las ejecuciones del algoritmo fueron
	satisfactorias en sus resultados, puede ser requerido mayor trabajo,
	tanto en la optimización del controlador (aumentando probablemente
	la expresividad de la representación), como en la evaluación de la
	función de aptitud. Ellos muestran un intento al respecto, premiando
	el movimiento oscilatorio, con lo cual obtienen un 80% de intentos
	exitosos, aunque no obtienen una mejora en el tiempo global empleado
	en relación con el número de caminatas exitosas obtenidas. [CPG]
	[Neuronal]},
  timestamp = {2006.10.23}
}

