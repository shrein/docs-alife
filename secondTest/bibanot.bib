@ARTICLE{Park03Fuzzy-logic,
  author = {Park, J.H.},
  title = {Fuzzy-logic zero-moment-point trajectory generation for reduced trunk
	motions of biped robots},
  journal = {Fuzzy Sets Syst},
  year = {2003},
  volume = {134},
  pages = {189--203},
  number = {1},
  __markedentry = {[Juan Figueredo]},
  abstract = {Trunk motions are typically used in biped robots to stabilize the
	locomotion. However, they can be very large for some leg trajectories
	unless they are carefully designed. This paper proposes a fuzzy-logic
	zero-moment-point (ZMP) trajectory generator that would eventually
	reduce the swing motion of the trunk significantly even though the
	leg trajectory is casually designed, for example, simply to avoid
	obstacles. The fuzzy-logic ZMP trajectory generator uses the leg
	trajectory as an input. The resulting ZMP trajectory is similar to
	that of a human one and continuously moves forward in the direction
	of the locomotion. The trajectory of the trunk to stabilize the locomotion
	is determined by solving a differential equation with the ZMP trajectory
	and the leg trajectory known. The proposed scheme is simulated on
	a 7-DOF biped robot in the sagittal plane. The simulation results
	show that the ZMP trajectory generated by the proposed fuzzy-logic
	generator increases the stability of the locomotion and thus reduces
	the motion range of the trunk significantly. © 2002 Elsevier Science
	B.V. All rights reserved.},
  comment = {Cited By (since 1996): 5
	
	Export Date: 29 September 2006
	
	Source: Scopus
	
	CODEN: FSSYD
	
	doi: 10.1016/S0165-0114(02)00237-3},
  file = {C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Park03Fuzzy-logic.pdf:C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Park03Fuzzy-logic.pdf:PDF},
  issn = {01650114 (ISSN)},
  keywords = {Biped robot, Locomotion, Zero-moment-point, Algorithms, Biped locomotion,
	Computer simulation, Differential equations, Fuzzy sets, Trajectories,
	Biped robots, Robots},
  owner = {Vicepresideencia},
  review = {Park aplica la conceptualización de la marcha desde el punto de momento
	zero (ZMP), buscando, en vez de generar movimientos para cada una
	de las articulaciones del caminador bípedo, generar las trayectorias
	del ZMP dinámicamente de acuerdo al estado de las articulaciones
	de la cadera y la pierna en balanceo, usando un sistema de lógica
	difusa de generación de trayectoria del tronco. Así, genera un movimiento
	mucho más natural y con desplazamientos de cadera reducidos. Para
	calcular los movimientos de las articulaciones una vez hallada la
	ubicación del ZMP se utiliza la cinemática inversa del mecanismo
	y se aplica un control por par calculado (computed torque controller).
	Las trayectorias de las piernas son recibidas como entrada al sistema
	de generación de trayectorias.
	
	
	Es interesante observar la posibilidad de hacer modificaciones dinámicas
	de la marcha partiendo de conceptos generales aplicados con lógica
	difusa. El concepto podría ser extendido usando métodos más elaborados
	para generar también las trayectorias de las piernas dada una directiva
	general de movimiento. El control por par calculado podría ser remplazado
	por una técnica más robusta que se adapte a variaciones en el sistema.
	[Dinámico][Difuso]},
  timestamp = {2006.09.28}
}

