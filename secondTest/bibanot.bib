@MISC{Chaisukkosol01Automatic,
  author = {Chanchai Chaisukkosol and Prabhas Chongstitvatana},
  title = {Automatic Synthesis Of Robot Programs For A Biped Static Walker By
	Evolutionary Computation},
  howpublished = {Second Asian Symposium on Industrial Automation and Robotics},
  year = {2001},
  __markedentry = {[Juan Figueredo]},
  abstract = {This work proposes a method to synthesis robot programs by Evolutionary
	Computation (EC). The problem of biped robot walking is chosen to
	test the proposed method. Walking motion is divided into six stages
	and the evolution is carried out stage-by-stage. The locomotion is
	restricted to forward walking on the flat and smooth surface with
	static balance. The synthesis process composed of both simulation
	and the experiment with a real robot. The result of the experiment
	shows that various types of gaits are achievable and the biped walking
	is stable.},
  file = {C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\CHAISUKKOSOL01Automatic.pdf:C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\CHAISUKKOSOL01Automatic.pdf:PDF},
  owner = {Juan Figueredo},
  review = {Chaisukkosol y Chongstitvatana utlizan un algoritmo genético para
	generar una secuencia de caminata en equilibrio estático para un
	robot bípedo con contrapeso. El movimiento del robot se realiza a
	una velocidad de 1 paso cada 40 segundos, por lo que el sistema se
	encuentra en equilibrio estático y la proyección de su centro de
	gravedad siempre debe estar dentro del polígono de sustentación.
	La representación de cada individuo contiene la longitud de la cadena
	(según parece) y la serie de desplazamientos angulares para los diferentes
	motores. La evolución se realiza secuencialmente para cada una de
	las 6 fases que los autores definen, iniciando la evolución de una
	fase donde terminó el mejor individuo de la evolución de la fase
	anterior. Se usa una función de aptitud global que evalúa estabilidad
	sumada con una función particular que evalúa el desempaño para el
	objetivo de cada fase. El resultado de cada fase es comprobado con
	la ejecución en un modelo real.
	
	
	El artículo trata la caminata como un problema de estabilidad pasiva
	y, a pesar de que es un modelo poco preciso de la caminata y que
	ofrece velocidades de desplazamiento muy bajas, muestra una aplicación
	adicional de la computación evolutiva a la caminata bípeda. Sin embargo,
	su relevancia es limitada. [No Relevante] [Estático]},
  timestamp = {2006.10.08},
  url = {citeseer.ist.psu.edu/chaisukkosol01automatic.html}
}

