@INPROCEEDINGS{Yanase06Evolutionary,
  author = {Toshihiko Yanase and Hitoshi Iba},
  title = {Evolutionary motion design for humanoid robots},
  booktitle = {GECCO '06: Proceedings of the 8th annual conference on Genetic and
	evolutionary computation},
  year = {2006},
  pages = {1825--1832},
  address = {New York, NY, USA},
  publisher = {ACM Press},
  doi = {http://www.sinab.unal.edu.co:2335/10.1145/1143997.1144291},
  file = {C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\Papers\\ACM\\biped\\p1825-yanase IC.pdf:C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\Papers\\ACM\\biped\\p1825-yanase IC.pdf:PDF},
  isbn = {1-59593-186-4},
  location = {Seattle, Washington, USA},
  review = {Yanase e Iba muestran una implementación de un algoritmo evolutivo
	con interacción con el usuario para determinar secuencias de movimiento,
	así como otras implementaciones con funciones de aptitud específicas
	para alcanzar cietos objetivos. En el primer caso se presenta la
	optimización de una secuencia de movimiento por computación evolutiva
	interactiva (IEC), para la cual se genera un conjunto de alternativas
	para cada cuadro de movimiento (keyframe), siendo estas evaluadas
	por el usuario gráficamente y, aplicando operadores evolutivos en
	sucesivas iteraciones junto con el mecanismo de evaluación, se obtiene
	cada uno de los cuadros que ha de formar la secuencia. Posteriormente
	muestran como se pueden usar funciones de aptitud especialmente diseñadas,
	en conjunto con un simulador dinámico, para optimizar tanto el movimiento
	para sentarse como para patear un balón. Así mismo, hacen una breve
	referencia a la implementación de un algoritmo genético multiobjetivo
	(MOGA) para la solución de tales problemas.
	
	
	El detalle novedoso de este trabajo es la implementación interactiva
	de la función de aptitud, si bien puede presentar problemas de escalabilidad.
	También introduce versatilidad a la programación de robots bípedos,
	haciéndola accesible para personas sin conocimientos de robótica
	o programación. Los métodos de análisis de estabilidad son límitados
	y la incorporación de las restricciones del problema al algoritmo
	no son aún datisfactorias.[Trayectoria]}
}

