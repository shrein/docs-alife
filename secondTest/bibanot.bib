@ARTICLE{Jha05On-line,
  author = {Jha, R.K. and Singh, B. and Pratihar, D.K.},
  title = {On-line stable gait generation of a two-legged robot using a genetic-fuzzy
	system},
  journal = {Robotics and Autonomous Systems},
  year = {2005},
  volume = {53},
  pages = {15--35},
  number = {1},
  __markedentry = {[Juan Figueredo]},
  abstract = {Gait generation for legged vehicles has since long been considered
	as an area of keen interest by the researchers. Soft computing is
	an emerging technique, whose utility is more stressed, when the problems
	are ill-defined, difficult to model and exhibit large scale solution
	spaces. Gait generation for legged vehicles is a complex task. Therefore,
	soft computing can be applied to solve it. In this work, gait generation
	problem of a two-legged robot is modeled using a fuzzy logic controller
	(FLC), whose rule base is optimized offline, using a genetic algorithm
	(GA). Two different GA-based approaches (to improve the performance
	of FLC) are developed and their performances are compared to that
	of manually constructed FLC. Once optimized, the FLCs will be able
	to generate dynamically stable gait of the biped. As the CPU-time
	of the algorithm is found to be only 0.002 s in a P-III PC, the algorithm
	is suitable for on-line (real-time) implementations. © 2005 Elsevier
	B.V. All rights reserved.},
  comment = {Export Date: 14 September 2006
	
	Source: Scopus},
  file = {C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Jha2005.pdf:C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Jha2005.pdf:PDF},
  owner = {Vicepresideencia},
  review = {Jha et al. muestran el diseño de un controlador para ascención de
	escaleras de un robot bípedo modelado en 2D. Emplean dos controladores
	por lógica difusa (fuzzy logic controller) para mantener la estabilidad
	del robot según el criterio de margen de estabilidad dinámico (dinamic
	stability margin) basado en el punto de momento cero (ZMP). El primer
	controlador se encarga de mantener la estabilidad durante la fase
	de apoyo simple y el segundo de mantenerla en la fase de apoyo doble.
	Los controladores son, en una primera aproximación, diseñados manualmente,
	después los controladores obtenidos son optimizados usando algoritmos
	genéticos (AG), y en una tercera aproximación son diseñados enteramente
	por el AG. Los bajos tiempos de ejecución de la optimización sugieren
	su posible aplicación en tiempo real, pero es necesario implementar
	compensacones por control ZMP cuando el controlador no logra conservar
	la estabilidad.
	
	
	Constituye una alternativa facil de desarrollar a controladores analíticos
	ya diseñados para tal fin, y que puede proporcionar respuestas más
	suaves y naturales. Sería interesante buscar otros métodos por inteligencia
	computacional para apoyar al sistema en caso de fallo, así como un
	mecanismo de transición en la acción de los diferentes controladores.
	[Dinámico][Difuso]},
  timestamp = {2006.09.14},
  url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-25144524228&partner=40&rel=R5.0.}
}

@ARTICLE{Juang02Intelligent,
  author = {Juang, J.-G.},
  title = {Intelligent locomotion control on sloping surfaces},
  journal = {Information Sciences},
  year = {2002},
  volume = {147},
  pages = {229--243},
  number = {1-4},
  __markedentry = {[Juan Figueredo]},
  abstract = {A learning scheme is developed to generate robotic locomotion on different
	sloping surfaces. This scheme uses three neural networks: a neural
	network controller, a neural network emulator, and a slope information
	neural network. The neural network controller is pre-trained by a
	reference trajectory on a horizontal surface. The emulator is pre-trained
	to identify the robotic dynamics. The slope information neural network
	provides compensated control signals to the robot on different slope
	angles by using the control signals on the horizontal surface from
	the pre-trained controller. The training rule is a backpropogation
	algorithm with time delay. The proposed technique can generate gaits
	on different sloping surfaces by following a reference trajectory
	with desired step length, crossing height and walking speed. © 2002
	Elsevier Science Inc. All rights reserved.},
  comment = {Cited By (since 1996): 1
	
	Export Date: 9 October 2006
	
	Source: Scopus
	
	CODEN: ISIJB
	
	doi: 10.1016/S0020-0255(02)00266-9},
  file = {C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Juang02Intelligent.pdf:C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Juang02Intelligent.pdf:PDF},
  issn = {00200255 (ISSN)},
  keywords = {Gait synthesis, Intelligent control, Neural networks, Robotic locomotion,
	Algorithms, Backpropagation, Gait analysis, Motion control, Neural
	networks, Robot learning, Robotic locomotion, Intelligent control},
  owner = {Juan Figueredo},
  review = {Juang realiza una aplicación de un sistema de control por redes neuronales
	para la caminata bípeda en superficies inclinadas. El sistema de
	control consta de: un controlador neuronal encarnado por una red
	neuronal de propagación hacia adelante que genera las señales de
	torque en los actuadores, un estimador neuronal que consiste también
	en una red neuronal de propagación hacia adelante, y una tercera
	red con la misma topología que adiciona una señal de control de compensación
	cuando hay una inclinación en la superficie. El método de entrenamiento
	empleado es retropropagación con retardo en el tiempo. El primer
	paso realizado es la identificación del sistema con el estimador
	neuronal. Posteriormente, se entrena el controlador neuronal para
	que siga una trayectoria prediseñada sobre superficies planas, usando
	el estimador para propagar el error de vuelta hacia la red de control.
	Finalmente, se dejan fijas las dos redes previas y se entrena el
	compensador de inclinación acoplado con el sistema de control, usando
	de nuevo el estimador para propagar el error hacia atrás. El seguimiento
	de trayectoria obtenido, aún en superficies inclinadas, es satisfactorio.
	
	
	Se aborda un método tradicional de control por redes neuronales con
	estimador del modelo, empleando un algoritmo de entrenamiento para
	redes neuronales en entornos dinámicos. La aproximación empleada
	es relativamente sencilla y demuestra resultados satisfactorios y
	robustos. Permite la variación de varios parámetros, entre ellos
	longitud y altura alcanzadas en el paso, y velocidad de la marcha.
	[Destacado] [Dinámico][Neuronal]},
