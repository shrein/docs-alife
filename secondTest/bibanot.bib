@ARTICLE{Sabourin05Robustness,
  author = {Sabourin, C. and Bruneau, O.},
  title = {Robustness of the dynamic walk of a biped robot subjected to disturbing
	external forces by using CMAC neural networks},
  journal = {Rob Autom Syst},
  year = {2005},
  volume = {51},
  pages = {81--89},
  number = {2-3},
  __markedentry = {[Juan Figueredo]},
  abstract = {In this paper, we propose a control strategy allowing us to perform
	the dynamic walking gait of an under-actuated robot even if this
	one is subjected to destabilizing external disturbances. This control
	strategy is based on two stages. The first one consists of using
	a set of pragmatic rules in order to generate a succession of passive
	and active phases allowing us to perform a dynamic walking gait of
	the robot. The joint trajectories of this reference gait are learned
	by using neural networks. In the second stage, we use these neural
	networks to generate the learned trajectories during the first stage.
	The goal of the use of these neural networks is to increase the robustness
	of the control of the dynamic walking gait of this robot in the case
	of external disturbances. The first experimental results are also
	presented. © 2005 Elsevier B.V. All rights resrved.},
  comment = {Cited By (since 1996): 1
	
	Export Date: 29 September 2006
	
	Source: Scopus
	
	CODEN: RASOE
	
	doi: 10.1016/j.robot.2005.02.001},
  file = {C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Sabourin05Robustness.pdf:C\:\\Documents and Settings\\Vicepresideencia\\Escritorio\\jj\\MISC\\SemInves1\\Lista Final\\Sabourin05Robustness.pdf:PDF},
  issn = {09218890 (ISSN)},
  keywords = {CMAC neural networks, Dynamic walking gait, Robustness, Under-actuated
	biped robot, Asymptotic stability, Biped locomotion, Learning systems,
	Neural networks, Real time systems, Robustness (control systems),
	Trajectories, CMAC, Dynamic walking gait, Robustness, Under-actuated
	biped robot, Robots},
  owner = {Vicepresideencia},
  review = {Sabourin y Bruneau aplican un control basado en seguimiento de trayectorias
	generadas por una red neuronal CMAC para controlar la caminata bípeda.
	Inicialmente calculan un conjunto de reglas o fases, de tipo activo
	y pasivo, las cuales generan actuaciones parciales en las diferentes
	articulaciones de acuerdo a la configuración geométrica en la que
	se encuentra el robot caminador. Posteriormente entrenan una red
	CMAC para que generalice las trayectorias necesarias para generar
	la marcha, particularmente en lo que se refiere a la pierna en balanceo.
	La rodilla de la pierna de apoyo se bloquea y su ángulo es determinado
	por un control de alto nivel que, recibiendo una velocidad como parámetro,
	modifica el ángulo para obtener una caminata con la velocidad deseada.
	El seguimiento de las trayectorias generadas por el control de alto
	nivel (pierna de apoyo) y la red CMAC (pierna en balanceo) es implementado
	con un control PD. Se muestra la robustez del método sometiéndolo
	a perturbaciones en el terreno, cargas aplicadas y deslizamiento.
	
	
	La metodología de control es bastante simple y no requiere cálculos
	particularmente complejos. Sin embargo, se requiere primero diseñar
	un conjunto de fases activas y pasivas para caracterizar la marcha
	y de tal forma se introduce, primero, elementos subóptimos al patron
	de marcha, y segundo, esquemas relativamente rígidos de marcha que
	han de ser reconfigurados para obtener distintos comportamientos.
	El entrenamiento de la red CMAC parece ser bastante costoso y no
	puede ser realizado en tiempo real, lo cual hace dudar también de
	la utilidad del método para sistemas con mayor complejidad.[Dinámico]
	[Neuronal]},
  timestamp = {2006.09.28}
}

